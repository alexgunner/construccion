<div class="container">
  <div class="row">
    <div class="col-lg-4">
      <form class="reservation-search" {{action "searchReserve" on="submit"}}>
        <div class="form-group">
          <label>Ingrese el codigo de su reserva</label>
          {{input type="text" class="form-control" name="code" value=code}}
        </div>
        <div class="form-group">
          {{input type="submit" value="Buscar" class="btn btn-success"}}
        </div>
      </form>
    </div>
  </div>
</div>
